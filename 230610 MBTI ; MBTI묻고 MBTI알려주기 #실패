#include <stdio.h>

int main() 
{
    int a = 50 , b = 50, c = 50, d = 50;
    char an, M[0], B[0], T[0], I[0];
    
//MBTI 첫 번째 자리  묻기
    printf("당신은 E입니까. I입니까. \n E/I = ");
    an = getchar();
    while(getchar() != '\n');

    if (an == 'e' || an == 'E') 
    {
        a += 10;
    }
    
    else if (an == 'i' || an == 'I')
    {
        a -= 10;
    }
    
//MBTI 두 번째 자리 묻기
    printf("당신은 N입니까. S입니까. \n N/S = ");
    an = getchar();
    while(getchar() != '\n');
    
    if (an == 'n' || an == 'N') 
    {
        b += 10;
    }
    
    else if (an == 's' || an == 'S')
    {
        b -= 10;
    }
    
//MBTI 세 번째 자리 묻기
    printf("당신은 F입니까. T입니까. \n F/T = ");
    an = getchar();
    while(getchar() != '\n');
    
    if (an == 'f' || an == 'F') 
    {
        c += 10;
    }
    
    else if (an == 't' || an == 'T')
    {
        c -= 10;
    }
    
    
//MBTI 네 번째 자리 묻기
    printf("당신은 P입니까. J입니까. \n P/J = ");
    an = getchar();
    while(getchar() != '\n');
    
    if (an == 'p' || an == 'P') 
    {
        d += 10;
    }
    
    else if (an == 'j' || an == 'J')
    {
        d -= 10;
    }
    
//MBTI 첫 번째 자리 알려주기

    if (a>50)
    {
        M == 'E';
    }
    else 
    {
        M == 'I';
    }
//MBTI 두 번째 자리 알려주기
    if (b>50)
    {
        B == 'N';
    }
    else 
    {
        B == 'S';
    }
//MBTI 세 번째 자리 알려주기
    if (c>50)
    {
        T == 'F';
    }
    else 
    {
        T == 'T';
    }

//MBTI 네 번째 자리 알려주기
    if (d>50)
    {
        I == 'P';
    }
    else 
    {
        I == 'J';
    }
//MBTI 알려주기
    printf("당신의 MBTI는 %s%s%s%s입니다.", M, B, T, I);
    

    return 0;
}
