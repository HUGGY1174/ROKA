    if (ns == 100)
    {
        printf(" S ◁◁◁◁◁▶▶▶▶▶ N\n");
    }



    //FT 수치
        if (ft == 10)
    {
        printf(" T ◁◀◀◀◀▷▷▷▷▷ F\n");
    }
    
    if (ft == 20)
    {
        printf(" T ◁◁◀◀◀▷▷▷▷▷ F\n");
    }
        
    if (ft == 30)
    {
        printf(" T ◁◁◁◀◀▷▷▷▷▷ F\n");
    }
    
    if (ft == 40)
    {
        printf(" T ◁◁◁◁◀▷▷▷▷▷ F\n");
    }
    
    if (ft == 50)
    {
        printf(" T ◁◁◁◁◁▷▷▷▷▷ F\n");
    }
    
    if (ft == 60)
    {
        printf(" T ◁◁◁◁◁▶▷▷▷▷ F\n");
    }
    
    if (ft == 70)
    {
        printf(" T ◁◁◁◁◁▶▶▷▷▷ F\n");
    }
    
    if (ft == 80)
    {
        printf(" T ◁◁◁◁◁▶▶▶▷▷ F\n");
    }
    
    if (ft == 90)
    {
        printf(" T ◁◁◁◁◁▶▶▶▶▷ F\n");
    }

    if (ft == 100)
    {
        printf(" T ◁◁◁◁◁▶▶▶▶▶ F\n");
    }
    
    
    
        //PJ 수치
    if (pj == 10)
    {
        printf(" J ◁◀◀◀◀▷▷▷▷▷ P\n");
    }
    
    if (pj == 20)
    {
        printf(" J ◁◁◀◀◀▷▷▷▷▷ P\n");
    }
        
    if (pj == 30)
    {
        printf(" J ◁◁◁◀◀▷▷▷▷▷ P\n");
    }
    
    if (pj == 40)
    {
        printf(" J ◁◁◁◁◀▷▷▷▷▷ P\n");
    }
    
    if (pj == 50)
    {
        printf(" J ◁◁◁◁◁▷▷▷▷▷ P\n");
    }
    
    if (pj == 60)
    {
        printf(" J ◁◁◁◁◁▶▷▷▷▷ P\n");
    }
    
    if (pj == 70)
    {
        printf(" J ◁◁◁◁◁▶▶▷▷▷ P\n");
    }
    
    if (pj == 80)
    {
        printf(" J ◁◁◁◁◁▶▶▶▷▷ P\n");
